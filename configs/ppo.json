{
    "type": "ppo_agent",
    "states_preprocessing": [
        {
            "type": "image_resize",
            "width": 80,
            "height": 80
        },
        {
            "type": "grayscale"
        },
        {
            "type": "normalize"
        }, {
            "type": "sequence",
            "length": 4
        }
    ],
    "update_mode": {
        "unit": "episodes",
        "batch_size": 64,
        "frequency": 10
    },
    "memory": {
        "type": "latest",
        "include_next_states": false,
        "capacity": 5000
    },

    "step_optimizer": {
        "type": "adam",
        "learning_rate": 1e-6
    },

    "discount": 0.99,
    "distributions": null,
    "entropy_regularization": 0.01,

    "summarizer": {
        "directory": null,
        "labels": [],
        "seconds": 120
    },
    "execution": {
        "type": "single",
        "session_config": null,
        "distributed_spec": null
    }
}